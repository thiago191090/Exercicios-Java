package entities;

public class deposito {
	
	public int numconta;
	public String nome;
	public double saldoconta;
	
	public deposito(int numconta, String nome, double saldoconta) {
		this.numconta = numconta;
		this.nome = nome;
		this.saldoconta = saldoconta;
	}
	
	public deposito(int numconta, String nome) {
		this.numconta = numconta;
		this.nome = nome;
	}
	
	
	public void addSaldo(double deposit) {
		this.saldoconta += deposit;
	}
	
	public void revomeSaldo(double saque) {
		this.saldoconta = saldoconta - (saque + (saque * 0.05));
	}
}
